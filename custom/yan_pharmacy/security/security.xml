<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="yan_hms_pharmacy_category" model="ir.module.category">
            <field name="name">Parmacy</field>
            <field name="description">Manage your Company Documents.</field>
            <field name="sequence">11</field>
            <field name="parent_id" ref="yan_hims_start.module_category_hms"/>
        </record>

        <record id="group_hms_pharmacy_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="yan_hms_pharmacy_category"/>
        </record>

        <record id="group_hms_pharmacy_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="yan_hms_pharmacy_category"/>
            <field name="implied_ids" eval="[(4, ref('yan_pharmacy.group_hms_pharmacy_user')),(4, ref('yan_hims_start.group_hms_user')),(4, ref('account.group_account_invoice')),(4, ref('stock.group_stock_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
 
    </data>

</odoo>
