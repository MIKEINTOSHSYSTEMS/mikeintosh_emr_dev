<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_hms_patient" model="ir.sequence">
            <field name="name">Patient</field>
            <field name="code">hms.patient</field>
            <field name="prefix">PAT</field>
            <field name="padding">3</field>
            <field name="yan_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_yan_physician" model="ir.sequence">
            <field name="name">Physician</field>
            <field name="code">hms.physician</field>
            <field name="prefix">DR</field>
            <field name="padding">3</field>
            <field name="yan_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
