<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_hispitalization" model="ir.sequence">
            <field name="name">YAN Hospitalization</field>
            <field name="code">yan.hospitalization</field>
            <field name="prefix">HOSP</field>
            <field name="padding">3</field>
            <field name="yan_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_death_register" model="ir.sequence">
            <field name="name">Death Register</field>
            <field name="code">patient.death.register</field>
            <field name="prefix">DEAD</field>
            <field name="padding">3</field>
            <field name="yan_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
