<odoo>
    <data noupdate="1">

        <!-- Data for Admission Check List -->
        <record id="admission_checklist_data_01" model="inpatient.checklist.template">
            <field name="name">Admission Form Filled and Signed by Relative</field>
        </record>

        <record id="admission_checklist_data_02" model="inpatient.checklist.template">
            <field name="name">Patient File Prepared</field>
        </record>

        <record id="admission_checklist_data_03" model="inpatient.checklist.template">
            <field name="name">Share General Information &amp; Facilitis</field>
        </record>

        <record id="admission_checklist_data_04" model="inpatient.checklist.template">
            <field name="name">Having Medi-claim or Schedule Deposit</field>
        </record>

        <record id="admission_checklist_data_05" model="inpatient.checklist.template">
            <field name="name">Registration Card &amp; Fee</field>
        </record>

        <!-- Company Data -->
        <record id="base.main_company" model="res.company" >
            <field name="yan_hospitalization_usage_location_id" ref="stock.stock_location_customers"/>
            <field name="yan_hospitalization_stock_location_id" ref="stock.stock_location_stock"/>
        </record>

    </data>
</odoo>